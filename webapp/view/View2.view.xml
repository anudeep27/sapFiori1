<mvc:View controllerName="com.anudeep.project2.controller.View2"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title2}" showNavButton="true" navButtonPress=".onNavBack" >
        <content />
    </Page>
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormDisplay356"
			editable="false"
			layout="ResponsiveGridLayout"
			title="Employee Details"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="Name" />
				<Text id="nameText" text="{SupplierName}" />
				<Label text="Street/No." />
				<Text text="{Street} {HouseNumber}" />
				<Label text="ZIP Code/City" />
				<Text text="{ZIPCode} {City}" />
				<Label text="Country" />
				<Text id="countryText" text="{Country}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</mvc:View>
